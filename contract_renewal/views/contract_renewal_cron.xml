<odoo>
    <data noupdate="1">
        <record id="ir_cron_checks_contract_expiry" model="ir.cron">
            <field name="name">Check Contracts Expiry</field>
            <field name="model_id" ref="contract_renewal.model_contract_renewal"/>
            <field name="state">code</field>
            <field name="code">model._check_contract_expiry()</field>
            <field name="interval_type">days</field>
            <field name="interval_number">1</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>
    </data>




    <data noupdate="1">
        <record id="ir_cron_update_contract_status" model="ir.cron">
            <field name="name">Update Contract Status</field>
            <field name="model_id" ref="hr_contract.model_hr_contract"/>
            <field name="state">code</field>
            <field name="code">model.update_contract_status()</field>
            <field name="active" eval="True"/>
            <field name="interval_type">days</field>
            <field name="interval_number">1</field>
            <field name="numbercall" eval="-1"/>
        </record>
    </data>

</odoo>
